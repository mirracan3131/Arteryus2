{
  "version": "2025-11-01",
  "apps": [
    {
      "id": "efebaba-paper-1-20",
      "title": "Efe Baba Paper 1.20",
      "description": "Installs and runs PaperMC build for Minecraft 1.20 on Linux. Auto-fetches the latest build for this version.",
      "tags": [
        "minecraft",
        "paper",
        "java",
        "server",
        "1.20"
      ],
      "icon": "https://papermc.io/assets/images/papermc-logo.svg",
      "platforms": [
        "linux"
      ],
      "variables": {
        "MC_VERSION": "1.20",
        "MEMORY": "4G",
        "JAR_NAME": "paper.jar"
      },
      "install": {
        "linux": [
          "set -e",
          "mkdir -p server && cd server",
          "printf 'eula=true\\n' > eula.txt",
          "API=\"https://api.papermc.io/v2/projects/paper/versions/${MC_VERSION}/builds\"",
          "BUILD=$(curl -s \"$API\" | jq -r '.builds[-1].build')",
          "URL=\"https://api.papermc.io/v2/projects/paper/versions/${MC_VERSION}/builds/${BUILD}/downloads/paper-${MC_VERSION}-${BUILD}.jar\"",
          "curl -Lf \"$URL\" -o ${JAR_NAME}"
        ]
      },
      "start": {
        "linux": "java -Xms${MEMORY} -Xmx${MEMORY} -jar ${JAR_NAME} nogui"
      }
    },
    {
      "id": "efebaba-forge-1-20",
      "title": "Efe Baba Forge 1.20",
      "description": "Installs the official Forge server for Minecraft 1.20 on Linux using the latest installer available on Forge Maven.",
      "tags": [
        "minecraft",
        "forge",
        "java",
        "server",
        "1.20"
      ],
      "icon": "https://files.minecraftforge.net/static/media/logo.9c1a9a6b.svg",
      "platforms": [
        "linux"
      ],
      "variables": {
        "MC_VERSION": "1.20",
        "MEMORY": "4G"
      },
      "install": {
        "linux": [
          "set -e",
          "mkdir -p server && cd server",
          "printf 'eula=true\\n' > eula.txt",
          "META_URL=\"https://maven.minecraftforge.net/net/minecraftforge/forge/maven-metadata.xml\"",
          "VER=$(curl -s $META_URL | sed -n 's:.*<version>\\(.*\\)</version>.*:\\1:p' | grep \"^${MC_VERSION}-\" | tail -n 1)",
          "INS=\"forge-${VER}-installer.jar\"",
          "curl -Lf \"https://maven.minecraftforge.net/net/minecraftforge/forge/${VER}/${INS}\" -o ${INS}",
          "java -jar ${INS} --installServer"
        ]
      },
      "start": {
        "linux": "bash run.sh"
      }
    },
    {
      "id": "efebaba-fabric-1-20",
